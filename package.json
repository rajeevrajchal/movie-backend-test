{
	"name": "movie-backend",
	"version": "1.0.0",
	"main": "./build/index.js",
	"license": "MIT",
	"scripts": {
		"build-ts": "tsc",
		"postinstall": "npm run build-ts",
		"start": "npm run serve",
		"serve": "node build/index.js",
		"watch-node": "nodemon build/index.js",
		"watch-ts": "tsc -w",
		"dev": "nodemon src/index.ts",
		"test": "env TS_NODE_COMPILER_OPTIONS='{\"module\": \"commonjs\" }' mocha -r ts-node/register './src/test/**/*.ts' --timeout=30000000"
	},
	"devDependencies": {
		"@types/cors": "^2.8.12",
		"@types/express": "^4.17.13",
		"@types/lodash": "^4.14.178",
		"@types/node": "^17.0.0",
		"nodemon": "^2.0.15",
		"prettier": "^2.5.1",
		"ts-node": "^10.4.0",
		"typescript": "^4.5.4"
	},
	"dependencies": {
		"@types/mocha": "^9.0.0",
		"axios": "^0.24.0",
		"chai": "^4.3.4",
		"chai-http": "^4.3.0",
		"cors": "^2.8.5",
		"express": "^4.17.2",
		"http": "^0.0.1-security",
		"lodash": "^4.17.21",
		"mocha": "^9.1.3",
		"node-cache": "^5.1.2"
	}
}
